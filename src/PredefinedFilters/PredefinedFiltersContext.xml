<?xml version="1.0" encoding="utf-8" ?>
<widget id="PredefinedFilters.widget.PredefinedFiltersContext" needsEntityContext="true" xmlns="http://www.mendix.com/widget/1.0/">
    <name>PredefinedFilters (Context)</name>
    <description>The description of this widget.</description>


    <icon></icon>




    <properties>
         <property key="isListView" type="boolean" required="true" defaultValue="false">
             <caption>ListView</caption>
             <category>General</category>
             <description>This widget (might) also work on ListViews. Use with caution</description>
         </property>
         <property key="showFilterIcon" type="boolean" required="true" defaultValue="true">
             <caption>Show Filter Icon</caption>
             <category>Appearance</category>
             <description>Should we include the filter icon on the button?</description>
         </property>
        <property key="gridName" type="string" required="true" defaultValue="grid1">
            <caption>Mendix Name</caption>
            <category>General</category>
            <description>The name of the Mendix Grid (or ListView)</description>
        </property>
        <property key="extraClass" type="string" required="false" defaultValue="">
            <caption>CSS Class for Buttons</caption>
            <category>Appearance</category>
            <description>Extra class(es) for buttons.</description>
        </property>
        <property key="showAll" type="boolean" required="true" defaultValue="true">
            <caption>Show All Button</caption>
            <category>Appearance</category>
            <description>Add an "All" button that shows all records</description>
        </property>
        <property key="baseEnum" type="attribute" required="true" isPath="optional" pathType="reference">
            <caption>Enumeration</caption>
            <category>General</category>
            <description>If you want to display a default filter based on an Enumeration value, enter that here.</description>
            <attributeTypes>
                <attributeType name="Enum" />
            </attributeTypes>
        </property>
        <property key="filters" type="object" required="false" isList="true">
            <caption>XPATH filters</caption>
            <category>General</category>
            <description></description>
            <properties>
                <property key="xpathstring" type="string" required="true">
                    <caption>XPATH query</caption>
                    <category>Filters</category>
                    <description>The XPATH query to run</description>
                </property>
                <property key="buttontext" type="string" required="true">
                    <caption>Button Text</caption>
                    <category>Filters</category>
                    <description>The Text to show on the Button</description>
                </property>
                 <property key="isdefault" type="boolean" required="true" defaultValue="false">
                     <caption>Is Default?</caption>
                     <category>Filters</category>
                     <description>If set to yes, this filter will be applied on pageload.</description>
                 </property>
                 <property key="enumkey" type="string" required="false" defaultValue="">
                     <caption>Enum key</caption>
                     <category>Filters</category>
                     <description>The Exact Enum key for which to show this filter as default (will fall back to `Is Default` value)</description>
                 </property>
            </properties>
        </property>
    </properties>
</widget>
